# Project display name (shown in dashboard)
PROJECT_NAME=MyProject

# Main repository path (the git repo you want to manage worktrees for)
MAIN_REPO_PATH=/path/to/your/repo

# Parent directory where worktrees will be created
WORKTREE_BASE_DIR=/path/to/worktree/parent

# Handler server port
HANDLER_PORT=3000

# Port range for worktree dev servers
PORT_RANGE_START=3001
PORT_RANGE_END=3099

# Health check interval (ms)
HEALTHCHECK_INTERVAL=10000

# Health check endpoint path (the endpoint your project exposes)
HEALTHCHECK_PATH=/api/healthz
